		if(respuesta == 0)
		{
			switch(opcion)
			{
			case 1:

				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
			}

		}
	}while(opcion!=5);

	return EXIT_SUCCESS;
}
